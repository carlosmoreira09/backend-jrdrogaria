{
  "name": "backend-shoppinglist",
  "version": "3.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts",
    "dev:preprod": "ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts dotenv_config_path=.env.preprod",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "audit-fix": "npm audit fix --force",
    "health-check": "node scripts/health-check.js",
    "fix-schema": "ts-node -r dotenv/config src/scripts/fix-schema.ts",
    "migrate-slugs": "ts-node -r dotenv/config src/scripts/migrate-tenant-slugs.ts",
    "migrate-data": "ts-node -r dotenv/config src/scripts/full-migration.ts",
    "migrate-schema": "ts-node -r dotenv/config src/scripts/run-migration.ts",
    "drop-tables": "ts-node -r dotenv/config src/scripts/drop-tables.ts",
    "seed": "ts-node -r dotenv/config src/scripts/seed-data.ts",
    "seed-admin": "ts-node -r dotenv/config src/scripts/seed-admin.ts"
  },
  "keywords": [],
  "author": "Carlos Moreira",
  "license": "ISC",
  "description": "Web Application to Handle with Suppliers and Products",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^5.0.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "nanoid": "^3.3.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.4.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8"
  }
}
